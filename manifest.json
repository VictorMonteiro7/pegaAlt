{
  "name": "Pega Alt",
  "description": "Pega textos alternativos do instagram e salva em um arquivo .txt",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "script.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html"
  }
}
